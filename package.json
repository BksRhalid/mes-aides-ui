{
  "name": "mes-aides",
  "description": "Simulateur de prestations sociales fran√ßaises.",
  "version": "0.0.1",
  "homepage": "https://github.com/sgmap/mes-aides-ui#readme",
  "author": "SGMAP <mes-aides@sgmap.fr>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgmap/mes-aides-ui.git"
  },
  "bugs": {
    "url": "https://github.com/sgmap/mes-aides-ui/issues"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2.14"
  },
  "main": "index.js",
  "scripts": {
    "start": "pm2 start ecosystem.json; echo '\nRead logs with `npm run logs`\n'",
    "dev": "pm2-dev ecosystem.json --watch",
    "dev-front": "webpack --watch  # should be combined with `dev`, but needs handling of concurrent interactive commands",
    "stop": "pm2 stop ecosystem.json && npm run stop-test-integration",
    "build": "rm -rf dist/* && webpack --optimize-minimize",
    "deploy": "pm2 deploy ecosystem.json production",
    "test": "npm run lint . && npm run test-unit test/unit && npm run test-integration",
    "pretest-integration": "NODE_ENV=test npm start && pm2 start node_modules/.bin/selenium && sleep 10 || echo 'Selenium supposedly started'",
    "test-integration": "watai test/integration",
    "test-unit": "NODE_ENV=test mocha --compilers js:babel-register",
    "stop-test-integration": "pm2 stop node_modules/.bin/selenium",
    "lint": "eslint --ignore-pattern=test/integration/* --ignore-pattern=dist",
    "logs": "pm2 logs",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "array.from": "^1.0.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-check-es2015-constants": "^6.7.2",
    "babel-plugin-transform-es2015-arrow-functions": "^6.5.2",
    "babel-plugin-transform-es2015-block-scoping": "^6.7.1",
    "babel-plugin-transform-es2015-classes": "^6.6.5",
    "babel-plugin-transform-es2015-computed-properties": "^6.6.5",
    "babel-plugin-transform-es2015-destructuring": "^6.5.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.5.2",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "babel-plugin-transform-es2015-shorthand-properties": "^6.5.0",
    "babel-plugin-transform-es2015-spread": "^6.6.5",
    "babel-plugin-transform-es2015-template-literals": "^6.6.5",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-register": "^6.5.1",
    "es6-promise": "^3.1.2",
    "expose-loader": "^0.7.1",
    "git-rev": "^0.2.1",
    "handlebars": "^3.0.3",
    "hapi": "^9.0.3",
    "inert": "^3.0.1",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.4.0",
    "json-loader": "^0.5.4",
    "moment": "^2.10.6",
    "object-path-immutable": "^0.3.0",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "semantic-ui-button": "^2.0.8",
    "semantic-ui-card": "^2.0.8",
    "semantic-ui-container": "^2.0.8",
    "semantic-ui-form": "^2.0.8",
    "semantic-ui-grid": "^2.0.8",
    "semantic-ui-header": "^2.0.8",
    "semantic-ui-icon": "^2.0.8",
    "semantic-ui-image": "^2.0.8",
    "semantic-ui-input": "^2.0.8",
    "semantic-ui-loader": "^2.1.7",
    "semantic-ui-message": "^2.0.8",
    "semantic-ui-reset": "^2.0.8",
    "semantic-ui-segment": "^2.0.8",
    "semantic-ui-site": "^2.0.8",
    "vision": "^4.0.1",
    "webpack": "^1.12.10"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "expect.js": "^0.3.1",
    "fetch-mock": "^4.1.1",
    "mocha": "^2.3.2",
    "pm2": "^1.0.0",
    "selenium-server": "^2.47.1",
    "watai": "^0.7.0"
  },
  "babel": {
    "plugins": [
      "check-es2015-constants",
      "transform-es2015-arrow-functions",
      "transform-es2015-block-scoping",
      "transform-es2015-classes",
      "transform-es2015-computed-properties",
      "transform-es2015-destructuring",
      "transform-es2015-modules-commonjs",
      "transform-es2015-parameters",
      "transform-es2015-shorthand-properties",
      "transform-es2015-spread",
      "transform-es2015-template-literals",
      "transform-object-assign"
    ]
  }
}
